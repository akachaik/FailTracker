@model FailTracker.Web.Controllers.IssueSummaryViewModel[]

<table class="table table-bordered table-striped table-condensed">
    <thead>
        <tr>
            <td>Subject</td>
            <td>Created At</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var issue in Model)
        {
            <tr>
                <td><a href="@Url.Action("View", "Issue", new { id = issue.IssueId})">@issue.Subject</a></td>
                <td>@issue.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>